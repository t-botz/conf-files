[user]
    name = Tibo
    email = delor.thibault@gmail.com

[alias]
    co = checkout
    br = branch
    ci = commit
    st = status

[format]
    pretty = format:%C(yellow)%h %Cgreen%<(14,trunc)%cr %<(14,trunc)%C(cyan)%aN %Creset%s

[fetch]
    prune = true
    pruneTags = true
[push]
    default = current
    autoSetupRemote = true
